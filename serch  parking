    ];

    function searchData() {
      const input = document.getElementById("search").value.trim();
      const resultDiv = document.getElementById("result");

      if (input.length !== 4) {
        resultDiv.innerHTML = "⚠️ 4자리 숫자를 입력해주세요.";
        return;
      }

      const found = carData.find(car => car.번호 === input);

      if (found) {
        resultDiv.innerHTML = `
          <p>✅ 번호 <strong>${input}</strong> 존재합니다.</p>
          <table>
            <tr><th>번호</th><th>주차스티커</th><th>색상</th><th>차종</th></tr>
            <tr>
              <td>${found.번호}</td>
              <td>${found.주차스티커}</td>
              <td>${found.색상}</td>
              <td>${found.차종}</td>
            </tr>
          </table>
        `;
      } else {
        resultDiv.innerHTML = `❌ 해당 번호는 등록되어 있지 않습니다.`;
      }
    }
  </script>
</body>
</html>
